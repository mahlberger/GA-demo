{
  "arrowParens": "always",
  "bracketSpacing": true,
  "embeddedLanguageFormatting": "auto",
  "htmlWhitespaceSensitivity": "strict",
  "insertPragma": false,
  "jsxBracketSameLine": true,
  "jsxSingleQuote": false,
  "printWidth": 120,
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "requirePragma": false,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
  "importOrder": [
    "^react",
    "^@apollo/(.*)$",
    "^@/(.*)$",
    "^generated/(.*)$",
    "^components/(.*)$",
    "^views/(.*)$",
    "^[./]",
    ""
  ],
  "importOrderSeparation": true
}
